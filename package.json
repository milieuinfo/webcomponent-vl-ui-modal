{
  "name": "vl-ui-modal",
  "version": "1.0.1",
  "description": "Gebruik de modal dialoog om de gebruiker te informeren over onbewaarde wijzigingen wanneer hij de pagina verlaat. Een modal dialoog vraagt de aandacht van e gebruiker en verplicht de gebruiker om actie te ondernemen voordat de site verder gebruikt kan worden.",
  "main": "vl-modal.js",
  "author": "DIDM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-modal.git"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-modal#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "scripts": {
    "postinstall": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "test-debug": "wct -l chrome,firefox --npm -p",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-modal.js -- *.src.js",
    "release:prepare:imports": "npm run release:prepare:imports:core && npm run release:prepare:imports:dist",
    "release:prepare:imports:core": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-modal.js",
    "release:prepare:imports:dist": "replace '../dist/modal.js' '/node_modules/vl-ui-modal/dist/modal.js' vl-modal.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-modal/style.css' vl-modal.js",
    "release:prepare:commit": "git add -f vl-modal.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-action-group": "1.0.9",
    "vl-ui-button": "1.0.10",
    "vl-ui-core": "1.4.3",
    "vl-ui-icon": "2.0.5"
  },
  "peerDependencies": {
    "vl-ui-core": "^1.4.2",
    "vl-ui-button": "^1.0.8",
    "vl-ui-icon": "^2.0.4",
    "vl-ui-action-group": "^1.0.7"
  },
  "devDependencies": {
    "@govflanders/vl-ui-core": "3.7.2",
    "@govflanders/vl-ui-modal": "3.7.2",
    "@govflanders/vl-ui-util": "3.7.2",
    "http-server": "0.11.1",
    "np": "3.0.4",
    "replace": "1.1.0",
    "sinon": "7.3.2",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "vl-ui-util": "1.1.3",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  }
}
